alog v-model=\"dialog\" width=\"800px\">\r\n      <v-card elevation=\"15\" class=\"special-scroll\">\r\n        <v-card-title>Nouvelle installation</v-card-title>\r\n        <v-container>\r\n          <v-form class=\"mx-5\">\r\n            <v-row>\r\n              <v-col cols=\"12\" md=\"12\">\r\n                <v-text-field v-model=\"editedInstallation.label\" label=\"Solution\" required></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"12\">\r\n                <v-text-field v-model=\"editedInstallation.description\" label=\"Type d'installation à concevoir\"></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n          </v-form>\r\n          <v-row v-show=\"editedInstallation.label\">\r\n            <v-col cols=\"12\">\r\n              <v-chip light class=\"ma-2 elevation-5 white--text\" color=\"amber darken-3\" @click=\"save\">\r\n                <v-avatar left><v-icon small>mdi-telegram</v-icon></v-avatar>\r\n                <span>Enrégistrer</span>\r\n              </v-chip>\r\n            </v-col>\r\n          </v-row>\r\n        </v-container>\r\n      </v-card>\r\n    </v-dialog>\r\n    <v-layout wrap class=\"px-0\">\r\n      <v-flex sm12 xs12 md12 lg12>\r\n        <v-card tile class=\"mx-auto mt-8\">\r\n          <v-sheet class=\"v-sheet--offset mx-auto py-3 px-3\" color=\"amber darken-4\" elevation=\"10\" max-width=\"calc(100% - 32px)\">\r\n            <v-card-text class=\"pt-0\">\r\n              <div class=\"title font-weight-thin mb-0 white--text\">\r\n                Systèmes de installation\r\n                <div class=\"float-right\" style=\"position: relative; top: 35px\">\r\n                  <v-chip light class=\"ma-2 elevation-5 float-right white--text\" color=\"amber darken-3\" @click=\"dialog = !dialog\">\r\n                    <v-avatar left><v-icon small>mdi-plus</v-icon></v-avatar>\r\n                    <span>Ajouter</span>\r\n                  </v-chip>\r\n                </div>\r\n              </div>\r\n            </v-card-text>\r\n          </v-sheet>\r\n\r\n          <v-card-text class=\"pt-0\" v-if=\"installations\">\r\n            <v-data-table :headers=\"headers\" :items=\"installations\" :items-per-page=\"10\" class=\"mb-3\">\r\n              <template v-slot:item.action=\"{ item }\">\r\n                <v-chip style=\"float: right\" outlined light class=\"ma-2 elevation-5\" color=\"red darken-3\" @click=\"deleteItem(item)\">\r\n                  <v-avatar left><v-icon small>mdi-delete-outline</v-icon></v-avatar>\r\n                  <span>Supprimer</span>\r\n                </v-chip>\r\n                <v-chip style=\"float: right\" outlined light class=\"ma-2 elevation-5\" color=\"amber darken-3\" @click=\"editItem(item)\">\r\n                  <v-avatar left><v-icon small>mdi-pencil-outline</v-icon></v-avatar>\r\n                  <span>Modifier</span>\r\n                </v-chip>\r\n              </template>\r\n              <template v-slot:no-data>\r\n                <p class=\"dark mt-15\">Aucune donnée enrégistrée pour le moment</p>\r\n              </template>\r\n            </v-data-table>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-flex>\r\n    </v-layout>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      currentUser: null,\r\n      editedIndex: -1,\r\n      dialog: false,\r\n      editedInstallation: {\r\n        id: null,\r\n        label: null,\r\n        description: null\r\n      },\r\n      newInstallation: {\r\n        id: null,\r\n        label: null,\r\n        description: null\r\n      },\r\n      installations: [],\r\n      headers: [\r\n        { text: 'Type Installation', value: 'label' },\r\n        { text: 'Type d\\'installation à concevoir', value: 'description' },\r\n        { text: '', value: 'action', sortable: false }\r\n      ],\r\n    }\r\n  },\r\n  watch: {\r\n    dialog (val) {\r\n      val || this.close()\r\n    },\r\n  },\r\n  methods: {\r\n    editItem (item) {\r\n      this.editedIndex = this.installations.indexOf(item)\r\n      this.editedInstallation = Object.assign({}, item)\r\n      this.dialog = true\r\n    },\r\n    deleteItem (item) {\r\n      const index = this.installations.indexOf(item)\r\n      if (confirm('Êtes vous sûr de vouloir supprimer ?')) {\r\n        this.installations.splice(index, 1)\r\n        this.saveToLocal()\r\n      }\r\n    },\r\n    close () {\r\n      this.dialog = false\r\n      setTimeout(() => {\r\n        this.editedInstallation = Object.assign({}, this.newInstallation)\r\n        this.editedIndex = -1\r\n      }, 300)\r\n    },\r\n    saveToLocal () {\r\n      let data = JSON.stringify(this.installations)\r\n      window.localStorage.setItem('jules-app-installation', data)\r\n    },\r\n    save () {\r\n      if (this.editedIndex > -1) {\r\n        Object.assign(this.installations[this.editedIndex], this.editedInstallation)\r\n      } else {\r\n        this.installations.push({\r\n          id: Math.floor(new Date () * Math.random() * Math.floor(9)),\r\n          label: this.editedInstallation.label,\r\n          description: this.editedInstallation.description\r\n        })\r\n      }\r\n      this.saveToLocal()\r\n      this.close()\r\n    }\r\n  },\r\n  mounted () {\r\n    this.currentUser = JSON.parse(window.localStorage.getItem('jules-user-connected'))\r\n    if (window.localStorage.getItem('jules-app-installation') !== null && window.localStorage.getItem('jules-app-installation').length > 0) { this.installations = JSON.parse(window.localStorage.getItem('jules-app-installation')) }\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .v-sheet--offset {\r\n    top: -24px;\r\n    position: relative;\r\n  }\r\n  .card-prepend {\r\n    width: 90%;\r\n    margin-left: 5%;\r\n\r\n    .card-offset {\r\n      height: 200px;\r\n    }\r\n\r\n    .card-content {\r\n      position: absolute;\r\n      top: 0;\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/views/app/settings"}]}